PROGRAM ASSINGMENT_5_26
    REAL:: AM=500.00,RT=0.07,N=10
    DO I=1,N
        AM=AM+(AM*RT)
        PRINT*,"AMOUNT=",AM," in ",I," years"
    END DO
END PROGRAM


